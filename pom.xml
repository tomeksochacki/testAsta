        <maven.compiler.target>11</maven.compiler.target>
        <io.qameta.allure>2.14.0</io.qameta.allure>
        <io.github.bonigarcia>5.0.1</io.github.bonigarcia>
        <org.seleniumhq.selenium>3.141.59</org.seleniumhq.selenium>
        <org.testng>7.4.0</org.testng>
        <log4j-core>2.13.0</log4j-core>
        <log4j-api>2.13.0</log4j-api>
